# For numeric inputs
# Any integer is supported.
# Looks like this: Setting: Value [-step] [=] [+step] [...]
# Step is replaced with the value from the step input
# When [-] is clicked, removes the step amount from the setting's value, and [+] does the opposite.
# When [=] is clicked, it puts the command '/scoreboard players set (id) (objective) 0' into the player's chatbox. The user then can set the value to whatever they want.

template numeric_input {
    with name:js desc:js objective:word id:word step:float disabled condition disabledcondition:js returnfunc:word {
        execute <%disabledcondition.concat(disabledcondition != null ? " " : "")%>run {
            tellraw @s [{text:"<%name%>",color:"gray",hover_event:{action:"show_text",value:["<%desc%>","\n",{text:"This setting is locked.", color:"gray"},"\n",{text:"<%objective%>/<%id%>",color:"dark_gray",italic:true}]}},{text:": ",color:"white"},{score:{name:"<%id%>",objective:"<%objective%>"},color:"green"}," ",{text:"[-<%step%>]",color:"dark_red",hover_event:{action:"show_text",value:"This setting is locked"}}," ",{text:"[=]",color:"gold",hover_event:{action:"show_text",value:"This setting is disabled"}}," ",{text:"[+<%step%>]",color:"dark_green",hover_event:{action:"show_text",value:"This setting is disabled"}}]
        } else execute run {
            tellraw @s [{text:"<%name%>",color:"aqua",hover_event:{action:"show_text",value:["<%desc%>","\n",{text:"<%objective%>/<%id%>",color:"dark_gray",italic:true}]}},{text:": ",color:"white"},{score:{name:"<%id%>",objective:"<%objective%>"},color:"green"}," ",{text:"[-<%step%>]",color:"red",hover_event:{action:"show_text",value:"Click to subtract <%step%>"},click_event:{action:"run_command",command:"/function z_configmenu:run_and_return {cmd:\"scoreboard players remove <%id%> <%objective%> <%step%>\",return:\"function <%returnfunc%>\"}"}}," ",{text:"[=]",color:"yellow",hover_event:{action:"show_text",value:"Click to set a value manually (reopen the config manually after doing so)"},click_event:{action:"suggest_command",command:"/scoreboard players set <%id%> <%objective%> 0"}}," ",{text:"[+<%step%>]",color:"green",hover_event:{action:"show_text",value:"Click to add <%step%>"},click_event:{action:"run_command",command:"/function z_configmenu:run_and_return {cmd:\"scoreboard players add <%id%> <%objective%> <%step%>\",return:\"function <%returnfunc%>\"}"}}]
