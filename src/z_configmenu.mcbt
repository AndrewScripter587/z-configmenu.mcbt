# An input that only has [...] for a button. Intended for having separate categories of settings in their own config page for better organization and a more compact main config screen.
# Looks like this: Category: [...]

template submenu {
    with name:js desc:js subconfig:word {
        tellraw @s [{text:"<%name%>",color:"aqua",hover_event:{action:"show_text",value:["<%desc%>","\n",{text:"Submenu",color:"dark_gray",italic:true}]}},{text:": ",color:"white"},{text:"[...]",color:"white",hover_event:{action:"show_text",value:"This setting requires a separate menu. Click to open it."},click_event:{action:"run_command",command:"/function <%subconfig%>"}}]
    }

    with legacy name:js desc:js subconfig:word {
        tellraw @s [{"text":"<%name%>","color":"aqua","hoverEvent":{"action":"show_text","value":["<%desc%>","\n",{"text":"Submenu","color":"dark_gray","italic":true}]}},{"text":": ","color":"white"},{"text":"[...]","color":"white","hoverEvent":{"action":"show_text","value":"This setting requires a separate menu. Click to open it."},"clickEvent":{"action":"run_command","value":"/function <%subconfig%>"}}]
    }
}

# A simple value display. 
# Unlike the rest of the inputs in this menu, this isn't limited to just a scoreboard value, it also allows you to use a NBT data storage value or a constant value.
# Looks like: Depends
# Score: Name: Value
# Data storage: Name: NBT Value
# Constant: Name: (Whatever text or value you want here)

template value_display {
    with name:js desc:js constant value:raw {
        tellraw @s [{text:"<%name%>",color:"aqua",hover_event:{action:"show_text",value:["<%desc%>","\n",{text:"ValueDisplay:CustomValue",color:"dark_gray",italic:true}]}},{text:": ",color:"white"},{text:"<%value%>",color:"green"}]
    }
    with name:js desc:js score objective:word id:word {
        tellraw @s [{text:"<%name%>",color:"aqua",hover_event:{action:"show_text",value:["<%desc%>","\n",{text:"<%objective%>/<%id%>",color:"dark_gray",italic:true}]}},{text:": ",color:"white"},{score:{name:"<%id%>","objective":"<%objective%>"},color:"green"}]
    }    
    with name:js desc:js data storage storage:js path:js {
        tellraw @s [{text:"<%name%>",color:"aqua",hover_event:{action:"show_text",value:["<%desc%>","\n",{text:"Data Storage <%storage%>/<%path%>",color:"dark_gray",italic:true}]}},{text:": ",color:"white"},{storage:<%storage%>,nbt:"<%path%>",color:"green"}]
    }    


    with legacy name:js desc:js constant value:raw {
        tellraw @s [{"text":"<%name%>","color":"aqua","hoverEvent":{"action":"show_text","value":["<%desc%>","\n",{"text":"ValueDisplay:CustomValue","color":"dark_gray","italic":true}]}},{"text":": ","color":"white"},{"text":"<%value%>","color":"green"}]
    }
    with legacy name:js desc:js score objective:word id:word {
        tellraw @s [{"text":"<%name%>","color":"aqua","hoverEvent":{"action":"show_text","value":["<%desc%>","\n",{"text":"<%objective%>/<%id%>","color":"dark_gray","italic":true}]}},{"text":": ","color":"white"},{"score":{"name":"<%id%>","objective":"<%objective%>"},"color":"green"}]
    }    
    with legacy name:js desc:js data storage storage:js path:js {
        tellraw @s [{"text":"<%name%>","color":"aqua","hoverEvent":{"action":"show_text","value":["<%desc%>","\n",{"text":"Data Storage <%storage%>/<%path%>","color":"dark_gray","italic":true}]}},{"text":": ","color":"white"},{"storage":"<%storage%>","nbt":"<%path%>","color":"green"}]
    }    

}

# A simple page bar.
# Useful for multiple pages in a config.
# The look of it depends on the amount of pages specified: [1][2][3][4]...
# Usage: pagebar (page amount) (prefix)
# where page amount is the amount of pages, and prefix is the path of the pages.
# Each button generated by this template will run the function (prefix)/page(button number)
# Example: setting the prefix to 'pack:config' will make buttons 1, 2, 3, ... run functions 'pack:config/page1', 'pack:config/page2', 'pack:config/page3', ...

template pagebar {
    with pages:int functionprefix:js {
        data remove storage z_configmenu:temp TextComponent
        REPEAT (1,pages) as i {
            data modify storage z_configmenu:temp TextComponent append value {text:"[<%i%>]",color:"gold",click_event:{action:"run_command",command:"/function <%functionprefix%>/page<%i%>"},hover_event:{action:"show_text",value:"Click to go to page <%i%>."}}
        }
        tellraw @s {storage:"z_configmenu:temp",nbt:"TextComponent",interpret:true}
    }
    with legacy pages:int functionprefix:js {
        data remove storage z_configmenu:temp TextComponent
        REPEAT (1,pages) as i {
            data modify storage z_configmenu:temp TextComponent append value {"text":"[<%i%>]","color":"gold","clickEvent":{"action":"run_command","value":"/function <%functionprefix%>/page<%i%>"},"hoverEvent":{"action":"show_text","value":"Click to go to page <%i%>."}}
        }
        tellraw @s {"storage":"z_configmenu:temp","nbt":"TextComponent","interpret":true}
    }
}

# Prints 1 or more empty lines.
# If used without arguments, prints a single empty line.
# If a number is provided, it will print that many lines.

template emptyline {
    with {
        tellraw @s ""
    }
    with amount:int {
        REPEAT (1, amount) as variable {
            tellraw @s ""
        }
    }
}

# A single button with a customizable name, color, and click command.

template single_button {
    with text:js color:word cmd:js {
        tellraw @s [{text:"[<%text%>]",color:<%color%>,click_event:{action:run_command,command:"<%cmd%>"}}]
    }
    with legacy text:js color:word cmd:js {
        tellraw @s [{"text":"[<%text%>]","color":"<%color%>","clickEvent":{"action":"run_command","value":"<%cmd%>"}}]
    }

}

# Exact same as /tellraw @s

template textline {
    with text:raw {
        tellraw @s <%text%>
    }
}

# Asks the user if they want to confirm an action.
# If the 'no' parameter is omitted, it will only show a confirm button. If the 'no' parameter is included, it also shows a cancel button.
# Clicking [Confirm] runs the command specified in the js input 'yes', and clicking [Cancel] runs the command specified in the js input 'no'.
# An optional note can be provided.
# Has a function macro version, for use cases where the template cannot be used normally and needs to use function macroes.

# The normal legacy parameter doesn't apply here, instead, you need to provide a boolean for the legacy input to use the legacy text components.
# This is because when this was added it was intended to only be used in my OldCombat Datapack, where this template used the js variable $1_21_4compile that oldcombat defines to determine whether it should use the legacy text components.
# I, however, just realized while adding these comments that that was a stupid decision and decided to temporarily change it to this.
# It will soon follow the normal legacy parameter when I get time to do that.

template confirmnoreturn {
    with yes:js legacy:word {
        tellraw @s ["<%context.namespace%> is asking if you're sure about this action."]
        tellraw @s ""
        IF (legacy) {
            tellraw @s ["<%spacing%>",{"text":"[Confirm]","color":"green","clickEvent":{"action":"run_command","value":'<%yes%>'}}]
        } ELSE {
            tellraw @s ["<%spacing%>",{text:"[Confirm]",color:green,click_event:{action:run_command,command:'<%yes%>'}}]
        }

    }
    with yes:js no:js legacy:word {
        tellraw @s ["<%context.namespace%> is asking if you're sure about this action."]
        tellraw @s ""
        IF (legacy) {
            tellraw @s ["<%spacing%>",{"text":"[Confirm]","color":"green","clickEvent":{"action":"run_command","value":'<%yes%>'}}," ",{"text":"[Cancel]","color":"red","clickEvent":{"action":"run_command","value":'<%no%>'}}]
        } ELSE {
            tellraw @s ["<%spacing%>",{text:"[Confirm]",color:green,click_event:{action:run_command,command:'<%yes%>'}}," ",{text:"[Cancel]",color:red,click_event:{action:run_command,command:'<%no%>'}}]
        }

    }

    with macro legacy:word {
        <%%
            spacing = "\\t".repeat(0);
        %%>
        $tellraw @s ["$(namespace) is asking if you're sure about this action."]
        tellraw @s ""
        IF (legacy) {
            $tellraw @s ["<%spacing%>",{"text":"[Confirm]","color":"green","clickEvent":{"action":"run_command","value":'$(yes)'}}," ",{"text":"[Cancel]","color":"red","clickEvent":{"action":"run_command","value":'$(no)'}}]
        } ELSE {
            $tellraw @s ["<%spacing%>",{text:"[Confirm]",color:green,click_event:{action:run_command,command:'$(yes)'}}," ",{text:"[Cancel]",color:red,click_event:{action:run_command,command:'$(no)'}}]
        }

    }
    with yes:js legacy:word note:raw {
        
        tellraw @s ["<%context.namespace%> is asking if you're sure about this action."]
        tellraw @s "<%note%>"
        IF (legacy) {
            tellraw @s ["<%spacing%>",{"text":"[Confirm]","color":"green","clickEvent":{"action":"run_command","value":'<%yes%>'}}]
        } ELSE {
            tellraw @s ["<%spacing%>",{text:"[Confirm]",color:green,click_event:{action:run_command,command:'<%yes%>'}}]
        }
    }
    with yes:js no:js legacy:word note:raw {
        tellraw @s ["<%context.namespace%> is asking if you're sure about this action."]
        tellraw @s "<%note%>"
        IF (legacy) {
            tellraw @s ["<%spacing%>",{"text":"[Confirm]","color":"green","clickEvent":{"action":"run_command","value":'<%yes%>'}}," ",{"text":"[Cancel]","color":"red","clickEvent":{"action":"run_command","value":'<%no%>'}}]
        } ELSE {
            tellraw @s ["<%spacing%>",{text:"[Confirm]",color:green,click_event:{action:run_command,command:'<%yes%>'}}," ",{text:"[Cancel]",color:red,click_event:{action:run_command,command:'<%no%>'}}]
        }

    }
    with macro legacy:word note {
        <%%
            spacing = "\\t".repeat(0);
        %%>
        $tellraw @s ["$(namespace) is asking if you're sure about this action."]
        $tellraw @s "$(note)"
        IF (legacy) {
            $tellraw @s ["<%spacing%>",{"text":"[Confirm]","color":"green","clickEvent":{"action":"run_command","value":'$(yes)'}}," ",{"text":"[Cancel]","color":"red","clickEvent":{"action":"run_command","value":'$(no)'}}]
        } ELSE {
            $tellraw @s ["<%spacing%>",{text:"[Confirm]",color:green,click_event:{action:run_command,command:'$(yes)'}}," ",{text:"[Cancel]",color:red,click_event:{action:run_command,command:'$(no)'}}]
        }

    }

}

